<?xml version='1.0' encoding='utf-8'?>
<widget id="com.messianicradio" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="HandleOpenUrl">
        <param name="ios-package" value="CDVHandleOpenURL" />
        <param name="onload" value="true" />
    </feature>
    <feature name="IntentAndNavigationFilter">
        <param name="ios-package" value="CDVIntentAndNavigationFilter" />
        <param name="onload" value="true" />
    </feature>
    <feature name="GestureHandler">
        <param name="ios-package" value="CDVGestureHandler" />
        <param name="onload" value="true" />
    </feature>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
        <param name="onload" value="true" />
    </feature>
    <feature name="HostedWebApp">
        <param name="ios-package" value="CDVHostedWebApp" />
        <param name="onload" value="true" />
    </feature>
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
    <feature name="BackgroundAudio">
        <param name="ios-package" onload="true" value="BackgroundAudio" />
        <param name="onload" value="true" />
    </feature>
    <name>Chavah</name>
    <description>The very best Messianic Jewish and Hebrew Roots music on the web</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="https://messianicradio.com/" />
    <access origin="*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation hap-rule="yes" href="https://messianicradio.com/*" />
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
